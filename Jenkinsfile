pipeline {
    agent any

    stages {
        stage('START') {
            steps {
                echo 'Starting pipeline...'
            }
        }

        stage('BUILD') {
            steps {
                echo 'Building pipeline...'
            }
        }
        
        stage('TEST') {
            steps {
                echo 'Testing pipeline...'
            }
        }
        
        stage('DEPLOY') {
            steps {
                echo 'Deploying pipeline...'
            }
        }
    }
}
